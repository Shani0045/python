    "from tkinter import*\n",
    "from tkinter import ttk\n",
    "root=Tk()\n",
    "root.geometry(\"600x400+300+200\")\n",
    "s=ttk.Style()\n",
    "theme=s.theme_names()\n",
    "print(theme)\n",
    "s.theme_use(\"classic\")\n",
    "\n",
    "e=ttk.Entry(root,show=\"\\u2022\")\n",
    "e.pack()\n",
    "\n",
    "btn=ttk.Button(root,text=\"Click Me\")\n",
    "btn.pack()\n",
    "\n",
    "\n",
    "root.mainloop()"
